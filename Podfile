# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

target 'TaxiAggregator-iOS' do
  use_frameworks!

  # Crashlytics
  pod 'Firebase/Core'
  pod 'Fabric'
  pod 'Crashlytics'

  # JSON
  pod 'ObjectMapper', '~> 3.4'
  pod 'SwiftyJSON', '~> 5.0'

  # Internet
  pod 'Starscream', '~> 3.0.2'

  # Store
  pod 'RealmSwift'

  # UI
  pod 'SnapKit', '~> 5.0.0'
 
  # General
  pod 'RxSwift', '~> 5'
  pod 'RxCocoa', '~> 5'
  pod "PromiseKit", "~> 6.8"
  
end
